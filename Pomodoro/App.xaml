<Application x:Class="Pomodoro.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Pomodoro"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="InterFont">pack://application,,,/Resources/Fonts/InterRegular.ttf#InterFont</FontFamily>
        <FontFamily x:Key="InterFontBold">pack://application,,,/Resources/Fonts/InterBold.ttf#InterFontBold</FontFamily>

        <FontFamily x:Key="Rajdhani">pack://application,,,/Resources/Fonts/RajdhaniBold.ttf#Rajdhani</FontFamily>
    </Application.Resources>
</Application>
